local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/jensonhirst/Orion/main/source'))()

local Window = OrionLib:MakeWindow({
	Name = "Frenzy | Universal Fe", 
	HidePremium = false, 
	SaveConfig = true, 
	IntroText = "Welcome!",
	ConfigFolder = "FrenzyConfig"
})

-------------------------------------
-- Main Tab
-------------------------------------
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddParagraph("Welcome!", "Thanks for using Frenzy | Troll. Make sure to have fun, harmlessly!")

MainTab:AddButton({
	Name = "Show Notification",
	Callback = function()
		OrionLib:MakeNotification({
			Name = "Frenzy",
			Content = "Script Loaded Successfully!",
			Image = "rbxassetid://4483345998",
			Time = 5
		})
	end    
})

MainTab:AddButton({
	Name = "Rejoin Game",
	Callback = function()
		local TeleportService = game:GetService("TeleportService")
		TeleportService:Teleport(game.PlaceId, game.Players.LocalPlayer)
	end
})

MainTab:AddButton({
	Name = "Server Hop",
	Callback = function()
		local HttpService = game:GetService("HttpService")
		local TeleportService = game:GetService("TeleportService")
		local PlaceId = game.PlaceId
		local servers = HttpService:JSONDecode(game:HttpGet(
			"https://games.roblox.com/v1/games/"..PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
		for _, server in pairs(servers.data) do
			if server.playing < server.maxPlayers then
				TeleportService:TeleportToPlaceInstance(PlaceId, server.id, game.Players.LocalPlayer)
				break
			end
		end
	end
})

local fpsBoost = false
MainTab:AddToggle({
	Name = "Toggle FPS Boost",
	Default = false,
	Callback = function(Value)
		fpsBoost = Value
		if Value then
			for _, v in pairs(game:GetDescendants()) do
				if v:IsA("Texture") or v:IsA("Decal") or v:IsA("ParticleEmitter") or v:IsA("Trail") then
					v.Enabled = false
				elseif v:IsA("BasePart") and v:FindFirstChildOfClass("SurfaceAppearance") then
					v:FindFirstChildOfClass("SurfaceAppearance"):Destroy()
				end
			end
		else
			game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
		end
	end
})

local sound = Instance.new("Sound", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
sound.Name = "FrenzyMusic"
sound.Looped = true
sound.Volume = 1

local musicId = ""

MainTab:AddTextbox({
	Name = "Music ID (Only numbers from roblox.com/library)",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		musicId = Value
	end
})

MainTab:AddButton({
	Name = "Play Music",
	Callback = function()
		if tonumber(musicId) then
			sound.SoundId = "rbxassetid://" .. musicId
			sound:Play()
		end
	end
})

MainTab:AddButton({
	Name = "Stop Music",
	Callback = function()
		sound:Stop()
	end
})

-------------------------------------
-- Teleport Tab
-------------------------------------
local TeleportTab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://6035198840",
	PremiumOnly = false
})

local selectedPlayer = nil

TeleportTab:AddDropdown({
	Name = "Select Player",
	Default = "None",
	Options = {},
	Callback = function(Value)
		selectedPlayer = game.Players:FindFirstChild(Value)
	end
})

local function refreshPlayers()
	local names = {}
	for _, v in pairs(game.Players:GetPlayers()) do
		if v.Name ~= game.Players.LocalPlayer.Name then
			table.insert(names, v.Name)
		end
	end
	return names
end

task.spawn(function()
	while true do
		task.wait(5)
		local options = refreshPlayers()
		TeleportTab:UpdateDropdown("Select Player", options)
	end
end)

TeleportTab:AddButton({
	Name = "Teleport to Player",
	Callback = function()
		if selectedPlayer and selectedPlayer.Character then
			game.Players.LocalPlayer.Character:MoveTo(selectedPlayer.Character:GetPivot().Position)
		end
	end    
})

TeleportTab:AddButton({
	Name = "Get TP Tool",
	Callback = function()
		local tool = Instance.new("Tool")
		tool.RequiresHandle = false
		tool.Name = "TP Tool"
		tool.Parent = game.Players.LocalPlayer.Backpack

		tool.Activated:Connect(function()
			local mouse = game.Players.LocalPlayer:GetMouse()
			local targetPos = mouse.Hit.Position
			game.Players.LocalPlayer.Character:MoveTo(targetPos)
		end)
	end
})

-------------------------------------
-- Tools Tab
-------------------------------------
local ToolsTab = Window:MakeTab({
	Name = "Tools",
	Icon = "rbxassetid://6031280882",
	PremiumOnly = false
})

local noclip = false
ToolsTab:AddToggle({
	Name = "Toggle Noclip",
	Default = false,
	Callback = function(state)
		noclip = state
	end    
})

game:GetService("RunService").Stepped:Connect(function()
	if noclip and game.Players.LocalPlayer.Character then
		for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = false
			end
		end
	end
end)

ToolsTab:AddSlider({
	Name = "WalkSpeed",
	Min = 16,
	Max = 200,
	Default = 16,
	Callback = function(val)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
	end    
})

ToolsTab:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 200,
	Default = 50,
	Callback = function(val)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = val
	end    
})

-------------------------------------
-- Player Tab
-------------------------------------
local PlayerTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://6031280997",
	PremiumOnly = false
})

PlayerTab:AddDropdown({
	Name = "Select Player",
	Default = "None",
	Options = refreshPlayers(),
	Callback = function(Value)
		selectedPlayer = game.Players:FindFirstChild(Value)
	end    
})

PlayerTab:AddButton({
	Name = "Spectate Player",
	Callback = function()
		if selectedPlayer and selectedPlayer.Character then
			game.Workspace.CurrentCamera.CameraSubject = selectedPlayer.Character:FindFirstChild("Humanoid")
		end
	end    
})

-------------------------------------
-- Game Tab (Chat Spam)
-------------------------------------
local GameTab = Window:MakeTab({
	Name = "Game",
	Icon = "rbxassetid://6034509993",
	PremiumOnly = false
})

local spamText = "Hello from Frenzy!"
local spamEnabled = false

GameTab:AddTextbox({
	Name = "Spam Message",
	Default = "Hello from Frenzy!",
	TextDisappear = false,
	Callback = function(Value)
		spamText = Value
	end	  
})

GameTab:AddToggle({
	Name = "Toggle Chat Spam",
	Default = false,
	Callback = function(Value)
		spamEnabled = Value
	end    
})

task.spawn(function()
	while true do
		if spamEnabled then
			game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(spamText, "All")
			wait(2)
		else
			wait(1)
		end
	end
end)

-- Init
OrionLib:Init() = "Hello Frenzy!",
	TextDisappear = true,
	Callback = function(input)
		OrionLib:MakeNotification({
			Name = "Frenzy AI",
			Content = "You said: " .. input .. "\nFrenzy says: That's cool!",
			Image = "rbxassetid://4483345998",
			Time = 5
		})
	end
})

-- Init
OrionLib:Init()
